<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.blg.datasource.demo.dao.BadGirlDao">

    <resultMap id="BaseResultMap" type="com.blg.datasource.demo.model.$BadGirl">
        <result column="id" jdbcType="BIGINT" property="id" />
        <result column="name" jdbcType="VARCHAR" property="name" />
    </resultMap>

    <select id="findAll" resultType="com.blg.datasource.demo.model.$BadGirl">
        select * from bad_girl where id = #{id}
    </select>

</mapper>